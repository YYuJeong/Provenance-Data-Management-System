                                    <td align=center>
                                        <%= values6[i] %>
                                    </td>
                                    <td align=center>
                                        <%= file_paths6[i] %>
                                    </td>
                                    <td align=center>
                                        <%= origins6[i] %>
                                    </td>
                                </tr>
                            <% } %>
                            </tbody>
                        </table>
